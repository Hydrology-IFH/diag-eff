[run]
branch = true
include = de/*
omit =
  tests/*
  */__init__.py
  # omit single files
  setup.py
  glob_eval.py
  make_index.py
  paper_code.py
  versioneer.py
  de/signatures.py
  de/_version.py
  de/misc.py

[report]
show_missing = True
# Regexes for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain if tests don't hit defensive assertion code:
    raise NotImplementedError
